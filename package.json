{
  "name": "bedrock_utils",
  "version": "0.0.10",
  "description": "Task system based on config files for your project",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:sendoushi/bedrock-utils.git"
  },
  "bin": "./bin/bedrock.js",
  "scripts": {
    "test": "npm run eslint-test",
    "eslint": "./node_modules/eslint/bin/eslint.js",
    "eslint-test": "npm run eslint -- ./modules/*",
    "help": "bin/bedrock.js",
    "example-create": "bin/bedrock.js init test/example_create/create.toml",
    "example-build": "bin/bedrock.js build test/example_build/build.toml dev",
    "example-sprite": "bin/bedrock.js build test/example_sprite/build.toml dev",
    "example-es6": "bin/bedrock.js build test/example_es6/build.toml dev",
    "example-typescript": "bin/bedrock.js run test/example_typescript/build.toml dev",
    "example-flow": "bin/bedrock.js run test/example_flow/build.toml dev",
    "example-server": "bin/bedrock.js run test/example_server/server.toml",
    "example-wordpress-init": "bin/bedrock.js init test/example_wordpress/server.toml",
    "example-wordpress-run": "bin/bedrock.js run test/example_wordpress/server.toml",
    "example-wordpress-stop": "bin/bedrock.js stop test/example_wordpress/server.toml",
    "example-wordpress-destroy": "bin/bedrock.js destroy test/example_wordpress/server.toml",
    "example-container-init": "bin/bedrock.js init test/example_container/server.toml",
    "example-container-run": "bin/bedrock.js run test/example_container/server.toml",
    "example-container-stop": "bin/bedrock.js stop test/example_container/server.toml",
    "example-container-destroy": "bin/bedrock.js destroy test/example_container/server.toml"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-defaults": "^8.0.2",
    "eslint-config-sds": "git://github.com/sendoushi/eslint-config-sds.git"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "colors": "^1.1.2",
    "glob": "^7.0.3",
    "joi": "^8.4.1",
    "sync-exec": "^0.6.2",
    "toml": "^2.3.0"
  }
}
